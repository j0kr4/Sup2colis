// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Package {
  id          Int      @id @default(autoincrement())
  nom         String
  adresse     String
  poids       Float
  destination String
  trackingId  String
}